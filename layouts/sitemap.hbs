<?xml version="1.0" encoding="UTF-8" ?>
{{#if site.sitemap.stylesheet}}
<?xml -stylesheet type="text/xsl" href="{{site.url}}{{site.sitemap.stylesheet}}" ?>
{{/if}}
<sitemapindex xmlns="http://www.sitemaps.org/schemas/sitemap/0.9">
  {{#list site.sitemap.urlsets}}
  <sitemap>
    <loc>{{site.url}}{{path}}</loc>
    <lastmod>{{now}}</lastmod>
  </sitemap>
  {{/list}}
</sitemapindex>